  "repository": "Haaretz/htz-frontend",
  "private": true,
  "scripts": {
    "build": "htz-scripts build",
    "clean": "htz-scripts clean",
    "build-all": "concurrently \"yarn workspace @haaretz/app-utils build\" \"yarn workspace @haaretz/graphql-server build\" \"yarn workspace @haaretz/htz-react-server build\" \"yarn workspace @haaretz/htz-user-utils build\" \"yarn workspace @haaretz/htz-components build\" \"yarn workspace @haaretz/login build\"",
    "dev": "cross-env concurrently \"cross-env NODE_ENV=development graphql-server\" \"cross-env NODE_ENV=development htz-react-server login | bunyan -L\"",
    "debug": "htz-react-server login --inspect-brk",
    "format": "htz-scripts format",
    "gc": "htz-scripts commitizen",
    "lint": "htz-scripts lint",
    "prepare": "npm run build",
    "start:pm2": "NODE_ENV=production pm2 start htz-react-server -- login",
    "start:prof": "NODE_ENV=production node --prof node_modules/.bin/htz-react-server finance",
    "start": "cross-env concurrently \"cross-env NODE_ENV=production graphql-server\" \"cross-env NODE_ENV=production htz-react-server login | bunyan -L\"",
    "test": "htz-scripts test",
    "update-scripts": "htz-scripts update-scripts"
  },
  "dependencies": {
